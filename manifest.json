{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_title": "Click here!"
    // "default_popup": "./front/background.html"
  },
  "background": {
    "scripts": ["./front/background.js"]
    // "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "background"
  ]
}
